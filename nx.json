{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "lint:js",
          "lint:css",
          "lint:html",
          "lint:all",
          "start",
          "build",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "lint:js": {
      "dependsOn": [
        "^lint:js"
      ]
    },
    "lint:css": {
      "dependsOn": [
        "^lint:css"
      ]
    },
    "lint:html": {
      "dependsOn": [
        "^lint:html"
      ]
    },
    "lint:all": {
      "dependsOn": [
        "^lint:all"
      ]
    },
    "start": {
      "dependsOn": [
        "^start"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    }
  },
  "affected": {
    "defaultBase": "develop"
  }
}
